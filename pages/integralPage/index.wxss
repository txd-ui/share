/* pages/integralPage/index.wxss */
.tips{
  font-size: 30rpx;
  font-family: '微软雅黑';
  text-indent: 2em;
  color: #fff;
  line-height: 60rpx;
  padding-left: 20rpx;
  box-sizing: border-box;
}
page{
  padding: 20rpx;
  display: flex;
  align-items: top;
  padding-top: 200rpx;
  padding-left: 0;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  position: relative;
}
.backImg{
  width: 100%;
  height: 100%;
  position: absolute;
  bottom: 0;
  z-index: -1;
}
.backImg image{
  width: 100%;
  height: 100%;
}