/* tab栏的切换 */

.tabColumn {
  width: 100%;
  height: 70rpx;
  background: #fff;
  border-radius: 20rpx;
  position: absolute;
  bottom: 10rpx;
  display: flex;
  align-items: center;
}

.van-tabs__scroll {
  background-color: '' !important;
  background-color: var(--tabs-nav-background-color, none) !important;
}

.van-hairline--top-bottom:after {
  border-width: 0 0 !important;
  box-sizing:border-box;
}
.van-notice-bar{
  height: 0 !important;
}
