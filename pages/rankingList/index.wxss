}

.white_top_block {
  width: 690rpx;
  background: #fff;
  height: 428rpx;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  margin-top: 118rpx;
  
  border-radius: 20rpx;
}

.userImg {
  width: 170rpx;
  height: 170rpx;
  border: 3rpx solid #efefef;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: -87rpx;
  display: flex;
  align-items: center;
  text-align: center;
  justify-content: center;
}

.userImg image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
}

.time_block {
  width: 610rpx;
  height: 42rpx;
