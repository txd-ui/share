/* pages/position/index.wxss */
.top_title{
  width: 100%;
  height: 75rpx;
  background: #f6f6f6;
  font-size: 36rpx;
  padding-left: 17rpx;
  box-sizing: border-box;
  font-family: '苹方粗体';
  line-height: 75rpx;
  font-weight: bold;
  color: #333;
}
.conter_img {
  position: absolute;
  top: 80rpx;
  bottom: 0;
  width: 100%;
  height: 92%;
}
.conter_img image{
  width: 100%;
  height: 100%;
}

 
.img {
  width: 100%;
  height: 480rpx;
}
.big_imgs{
  width: 663rpx;
  height: 750rpx;
  margin: 0 auto;
  position: absolute;
  top: 100rpx;
  z-index: 5;
  background: #d8d8d89a;
}
.conter_button{
  width: 260rpx;
  height: 260rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 6;
  border: 1px solid #fff; 
  padding-top: 40rpx;
  box-sizing: border-box;
  text-align: center;
}
.conter_button image{
  width: 160rpx;
  height: 160rpx;

}
.zhong{
   width: 663rpx;
  height: 750rpx;
  position: absolute;
   top: 100rpx;
  z-index: 6;

}
.zhong image{
  width: 100%;
  height: 100%;
}
.zhong .abc_img{
  position: absolute;
  width: 50rpx;
  height: 50rpx;
  top: 0;
  right: 0;
  z-index: 10;
}

.button_bom{
  width: 620rpx;
  height: 80rpx;
  background: #006fb9;
  font-family: '苹方粗体';
  font-size: 34rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  position: absolute;
  bottom: 50rpx;
  left: 50%;
  transform: translateX(-50%);
}