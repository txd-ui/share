page{
  width: 100%;
  height: 100%;
  background: #f3f3f3;
  position: relative;
  padding-top: 30rpx;
  box-sizing: border-box;
}
.one_block{
  width: 700rpx;
  height: 185rpx;
  background: #fbfbfb;
  box-shadow:  0 0 10rpx #e5e5e5;
  margin: 0 auto;
  border-radius: 15rpx;
  margin-bottom: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.left_Img{
  width: 230rpx;
  height: 133rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-right: 1px solid #c9c9c9;
}
.left_Img image{
  width: 163rpx;
  height: 112rpx;
}
.right_Font{
  flex: 1;
  display: flex;
  flex-direction: column;
  padding-left: 40rpx;
  font-size: 28rpx;
  font-family: '苹方粗体';
  color: #333;
}
.top_time{
  margin-top: 20rpx;
}